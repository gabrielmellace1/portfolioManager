{
  "name": "portfolio-visualizer-backend",
  "version": "1.0.0",
  "description": "Backend API for portfolio visualizer",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__/utils --verbose",
    "test:entities": "jest --testPathPattern=__tests__/entities --verbose",
    "test:services": "jest --testPathPattern=__tests__/services --verbose",
    "test:api": "jest --testPathPattern=__tests__/api --verbose",
    "test:all": "jest --verbose",
    "test:run": "ts-node src/__tests__/scripts/runTests.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "migrate": "node dist/migrations/MigrationRunner.js",
    "migrate:status": "node dist/migrations/MigrationRunner.js --status",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postbuild": "npm run type-check"
  },
  "dependencies": {
    "express": "^4.18.2",
    "tsoa": "^5.0.0",
    "typeorm": "^0.3.17",
    "sqlite3": "^5.1.6",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^21.5.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "reflect-metadata": "^0.1.13",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "redis": "^4.6.10"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.8.0",
    "@types/compression": "^1.7.5",
    "@types/redis": "^4.0.11",
    "typescript": "^5.2.2",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "eslint": "^8.52.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16",
    "rimraf": "^5.0.5"
  }
}
